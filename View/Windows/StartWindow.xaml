<Window x:Class="RefreshSalon.View.Windows.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RefreshSalon.View.Windows"
        mc:Ignorable="d"
        Title="StartWindow" Height="800" Width="1300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.2*" />
            <ColumnDefinition Width="0.8*" />
        </Grid.ColumnDefinitions>
        
        <Frame x:Name="frame" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="prozFrame" Grid.Column="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
